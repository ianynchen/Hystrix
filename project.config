[project]
	description = Hystrix is a latency and fault tolerance library designed to isolate points of access to remote systems, services and 3rd party libraries, stop cascading failure and enable resilience in complex distributed systems where failure is inevitable.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Yining Chen (ianynchen)
[access "refs/for/refs/*"]
	read = group user/Yining Chen (ianynchen)
	push = group user/Yining Chen (ianynchen)
	label-Code-Review = -2..+2 group user/Yining Chen (ianynchen)
	label-Verified = -1..+1 group user/Yining Chen (ianynchen)
	rebase = group user/Yining Chen (ianynchen)
	removeReviewer = group user/Yining Chen (ianynchen)
	submit = group user/Yining Chen (ianynchen)
[access "refs/heads/*"]
	read = group user/Yining Chen (ianynchen)
	create = group user/Yining Chen (ianynchen)
	push = +force group user/Yining Chen (ianynchen)
	pushMerge = group user/Yining Chen (ianynchen)
[access "refs/tags/*"]
	push = +force group user/Yining Chen (ianynchen)
	pushTag = group user/Yining Chen (ianynchen)
	pushSignedTag = group user/Yining Chen (ianynchen)
